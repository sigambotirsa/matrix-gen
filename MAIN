<body>
  <h1>Matrix Prediction Generator</h1>
  <div>
    <input type="number" id="ping" placeholder="Enter Ping (ms)" min="10" max="200">
    <button onclick="generateCoordinates()">Generate</button>
  </div>
  <div class="output" id="output"></div>

  <script>
    function generateCoordinates() {
      const ping = parseInt(document.getElementById('ping').value);
      if (isNaN(ping) || ping < 10 || ping > 200) {
        document.getElementById('output').textContent = 'Please enter a valid ping between 10-200ms.';
        return;
      }

      // Example formula for generating coordinates based on ping
      const x = (300 - ping) + Math.floor(Math.random() * 10);
      const y = ping + Math.floor(Math.random() * 10);

      document.getElementById('output').textContent = `Predicted X: ${x}, Y: ${y}`;
    }
  </script>
</body>
